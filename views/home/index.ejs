function sendMessage() {
  const message = messageInput.value;
  const messageElement = document.createElement('p');
  messageElement.textContent = message;
  chatArea.appendChild(messageElement);
  chatArea.style.display = 'block';

  // Scroll down to show the latest message
  chatArea.scrollTop = chatArea.scrollHeight;

  // Emit the message with client ID
  socket.emit('chat message', { id: clientId_, message: message });
  messageInput.value = '';
}

// Function to bind the event listener if it's not already bound
function bindEventListener() {
  if (!isEventListenerBound) {
    socket.on('chat message', (data) => {
      if (data.id !== clientId_) {
        const messageElement = document.createElement('p');
        messageElement.textContent = data.message;
        chatArea.appendChild(messageElement);
        chatArea.scrollTop = chatArea.scrollHeight; // Scroll down to show the latest message
      }
    });
    isEventListenerBound = true;
  }
}

// Call the function to bind the event listener
